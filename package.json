{
  "name": "newproject2",
  "version": "0.1.7",
  "description": "Sahat Al Majd Inventory System",
  "author": "Sahat Al Majd",
  "private": true,
  "main": "electron.js",
  "scripts": {
    "build": "next build",
    "dev": "next dev --webpack",
    "dev:turbo": "next dev --turbo",
    "lint": "eslint .",
    "start": "next start",
    "export": "node scripts/preexport.mjs && cross-env NEXT_PUBLIC_STATIC_EXPORT=true next build --webpack && node scripts/postexport.mjs",
    "tauri:dev": "tauri dev",
    "tauri:build": "tauri build",
    "desktop:build": "npm run export && npm run tauri:build",
    "electron:dev": "cross-env NODE_ENV=development electron .",
    "electron:build": "next build && electron-builder --win",
    "electron:build-portable": "next build && electron-builder --win portable",
    "electron:build-standalone": "npm run clean && npm run desktop:server-build && electron-builder --win"
  },
  "build": {
    "appId": "com.almajd.desktop2",
    "productName": "SAHATALMAJD",
    "compression": "store",
    "directories": {
      "buildResources": "public",
      "output": "release_new"
    },
    "files": [
      "electron.js",
      ".next/**/*",
      "public/**/*",
      "package.json",
      "!**/*.map"
    ],
    "asar": true,
    "win": {
      "target": [
        "nsis",
        "portable"
      ]
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "perMachine": false,
      "shortcutName": "SAHATALMAJD",
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true
    }
  },
